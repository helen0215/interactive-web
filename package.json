  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@webpack-cli/generators": "^2.4.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "filemanager-webpack-plugin": "^7.0.0-beta.0",
    "html-webpack-plugin": "^5.5.0",
