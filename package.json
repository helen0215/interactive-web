{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "webpack.config.js",
  "scripts": {
    "build": "npm run clean && webpack --node-env=production --config webpack.prod.js",
    "build:dev": "npm run clean && webpack --config webpack.dev.js",
    "build:prod": "npm run clean && webpack --node-env=production --config webpack.prod.js",
    "serve": "webpack serve --config webpack.dev.js",
    "clean": "rm -r -f ./dist ./index.html"
  },
